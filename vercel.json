{
  "version": 2,
  "name": "fourbyte",
  "builds": [
    {
      "src": "server/src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/src/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server/src/index.js"
    },
    {
      "src": "/health",
      "dest": "server/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/src/index.js": {
      "maxDuration": 30
    }
  }
}
